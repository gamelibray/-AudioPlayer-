config_version=5

[application]

config/name="AudioPlayer"
run/main_scene="res://Player/Scene/audio_player.tscn"
config/features=PackedStringArray("4.3", "Mobile")
boot_splash/bg_color=Color(0.866667, 0.866667, 0.866667, 1)
boot_splash/show_image=false
boot_splash/image="res://assets/texture/button/music.png"
config/icon="res://assets/texture/button/music.png"

[autoload]

Global="*res://Player/Script/Global.gd"

[display]

window/size/viewport_width=648
window/size/viewport_height=950
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=1
window/energy_saving/keep_screen_on.editor=true

